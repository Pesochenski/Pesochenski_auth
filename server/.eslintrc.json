{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google",
        "prettier",
        "plugin:prettier/recommended",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "require-jsdoc": 0,
      "no-unused-vars": 1
    }
}
