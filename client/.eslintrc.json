{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "eslint:recommended",
        "google",
        "plugin:prettier/recommended",
        "prettier",
        "plugin:import/recommended",
        "plugin:import/warnings",
        "plugin:import/errors"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier",
        "import"
    ],
    "rules": {
      "require-jsdoc": 0,
      "react/prop-types": "off",
      "no-unused-vars": 1
    }
}
